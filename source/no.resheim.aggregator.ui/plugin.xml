<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.ui"
            allowMultiple="false"
            class="no.resheim.aggregator.ui.views.RSSView"
            icon="icons/view.gif"
            id="no.resheim.aggregator.ui.RSSView"
            name="Feeds">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               id="no.resheim.aggregator.ui.RSSView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.ui.navigator.ProjectExplorer"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.views"
            defaultHandler="no.resheim.aggregator.ui.views.NextUnreadItemCommandHandler"
            id="no.resheim.aggregator.ui.nextUnread"
            name="Next Unread">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="no.resheim.aggregator.ui.context"
            name="Aggregator View"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="no.resheim.aggregator.ui.nextUnread"
            contextId="no.resheim.aggregator.ui.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="Ctrl+.">
      </key>
      <key
            commandId="no.resheim.aggregator.core.ui.delete"
            contextId="no.resheim.aggregator.ui.context"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="DEL">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:no.resheim.aggregator.ui.RSSView?after=additions">
         <command
               commandId="no.resheim.aggregator.core.ui.collapseAll"
               label="Collapse All"
               style="push">
         </command>
         <separator
               name="no.resheim.aggregator.ui.separator1"
               visible="true">
         </separator>
         <command
               commandId="no.resheim.aggregator.core.ui.addFeed"
               label="New Feed"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:no.resheim.aggregator.ui.RSSView?after=additions">
         <menu
               label="Collections">
            <visibleWhen
                  checkEnabled="false">
               <test
                     forcePluginActivation="true"
                     property="no.resheim.aggregator.multipleCollections">
               </test>
            </visibleWhen>
            <dynamic
                  class="no.resheim.aggregator.core.ui.commands.FeedCollectionMenu"
                  id="no.resheim.aggregator.ui.dynamic">
            </dynamic>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:no.resheim.aggregator.ui.RSSView?after=navigation">
         <command
               commandId="no.resheim.aggregator.ui.nextUnread"
               label="Next Unread">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:no.resheim.aggregator.ui.RSSView?after=modify">
         <command
               commandId="no.resheim.aggregator.core.ui.newFolder"
               label="New Folder">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="no.resheim.aggregator.core.ui.renameFolder"
               label="Rename">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="no.resheim.aggregator.core.ui.delete"
               label="Delete">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="no.resheim.aggregator.core.ui.emptyTrash"
               label="Empty Trash">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="no.resheim.aggregator.core.ui.update"
               label="Update">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="no.resheim.aggregator.core.ui.setRead"
               label="Mark As Read">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <menu
               label="Mark As">
            <dynamic
                  class="no.resheim.aggregator.core.ui.commands.MarkItemMenu"
                  id="no.resheim.aggregator.ui.markAsMenu">
            </dynamic>
         </menu>
         <command
               commandId="no.resheim.aggregator.core.ui.copyLinkCommand"
               label="Copy Link Location"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <separator
               name="properties"
               visible="true">
         </separator>
         <command
               commandId="no.resheim.aggregator.core.ui.properties"
               label="Properties">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
